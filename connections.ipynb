   "source": [
    "import json\n",
    "import snowflake.connector"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def conn(file):\n",
    "    with open(file,'r') as f:\n",
    "        cred=json.load(f)\n",
    "        \n",
    "    conn = snowflake.connector.connect(\n",
    "    user=cred[\"username\"],\n",
    "    password=cred[\"psw\"],\n",
    "    account=cred[\"account\"]\n",
    "    )\n",
    "    return conn"
   ]
  }
 ],
